<?php get_header(); ?><div id="main" <?php post_class('container') ?>>    <div class="row">        <h1>Blog</h1>        <div id="content-blog" class="col-md-7">            <?php            // The Query            $paged = (get_query_var('paged')) ? get_query_var('paged') : 1;            query_posts('cat=-7,-8&posts_per_page=5&orderby=date&order=DESC&paged='.$paged.'');            // The Loop            while (have_posts()) : the_post();                get_template_part('content', 'index');            endwhile;            // Reset Query            wp_reset_query(); ?>                        <nav>                <div class="nav-previous alignleft"><?php next_posts_link('&laquo; Posts antigos'); ?></div>                <div class="nav-next alignright"><?php previous_posts_link('Novos posts &raquo;'); ?></div>            </nav>        </div> <!-- #content-blog -->        <section id="sidebar" class="col-md-4 col-md-offset-1">            <?php get_sidebar(); ?>        </section><!-- #sidebar -->    </div><!-- .row--></div><?php get_footer(); ?>